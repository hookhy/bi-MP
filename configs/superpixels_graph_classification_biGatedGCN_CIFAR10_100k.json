{
    "gpu": {
        "use": true,
        "id": 6
    },
    
    "model": "biGatedGCN",
    "dataset": "CIFAR10",
    
    "out_dir": "out/superpixels_graph_classification/",
    
    "params": {
        "seed": 41,
        "epochs": 1000,
        "batch_size": 64,
        "init_lr": 0.001,
        "lr_reduce_factor": 0.5,
        "lr_schedule_patience": 10,
        "min_lr": 1e-5,
        "weight_decay": 0.0,
        "print_epoch_interval": 5,
        "max_time": 12
    },
    
    "net_params": {
        "L": 4,
        "hidden_dim": 70,
        "out_dim": 70,
        "residual": true,
        "edge_feat": true,
        "readout": "mean",
        "in_feat_dropout": 0.0,
        "dropout": 0.0,
        "batch_norm": true,
        "pool_ratio": 0.1,
        "pos_enc": false,
        "sigma": 1
    }
}